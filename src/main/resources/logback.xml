<configuration>
  <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%date{HH:mm:ss} [%-5level] %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="mk.hsilomedus.pn532" level="ERROR" />

  <!-- Set the root logger level to ERROR -->
  <root level="ERROR">
    <appender-ref ref="CONSOLE" />
  </root>
</configuration>